<body>
	<div class="tabel-buku1" style="background-color: white; margin: 20px; padding: 20px;">
		<table id="example" class="table table-striped table-bordered" style="width:100%; color:black">
			<thead >
				<tr style="background-color: black; color:white;">
					<th scope="col">Source</th>
					<th scope="col">Target</th>
					<!-- <th scope="col">Type</th> -->
					<th scope="col">Weight</th>
					<th scope="col">Book</th>
				</tr>
			</thead>
			<tbody>
				<% for(let data of book1) { %>
				<tr>
					<td><%= data.Source%></td>
					<td><%= data.Target%></td>
					<!-- <td><%= data.Type%></td> -->  
					<td><%= data.Weight%></td>
					<td><%= data.Book%></td>  
				</tr>
				<% } %>
			</tbody>
		</table>
	</div>
	<script>
    $(document).ready(function() {
    $('#example').DataTable({
    	"bInfo": false,
    	"bLengthChange": false,
    
    	language: {
        searchPlaceholder: "Cari karakter"
    	}
    	
    });
	});
	</script>

</body>
</html>