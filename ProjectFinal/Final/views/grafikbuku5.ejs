<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>Grafik Buku 5</title>
</head>
<body>
	<h1> Grafik Bar Buku 5 </h1>
	<div>
		<canvas id="myChart"></canvas>
	  </div>
	  <script src="https://cdnjs.cloudflare.com/ajax/libs/d3/7.7.0/d3.min.js" integrity="sha512-7RUyhA5mpSkxSGGdw1vL6ZLL2GzWPvf/aQ4M8cpXkZiSpCvWb5rgygK+VzUpyXx4vPc0ahrS53nymFbGOQ7ubg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
	  
	  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
	  
	  <script>
		const ctx = document.getElementById('myChart');
	  
		new Chart(ctx, {
		  type: 'bar',
		  data: {
			labels: ['Daenerys-Targaryen', 'Ramsay-Snow', 'Barristan-Selmy', 'Jon-Snow', 'Penny', 'Daenerys-Targaryen', 'Haldon', 'Bran-Stark', 'Daario-Naharis', 'Jon-Snowk'],
			datasets: [{
			  label: '# of Votes',
			  data: [96, 87, 75, 73, 59, 58, 54, 50, 50, 50],
			  borderWidth: 1
			}]
		  },
		  options: {
			scales: {
			  y: {
				beginAtZero: true
			  }
			}
		  }
		});
	  </script>
</body>
</html>